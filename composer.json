{
    "name": "q9team/driver-web",
    "license": "MIT",
    "description": "Web driver",
    "keywords": [],
    "homepage": "http://github.com/q9team/driver-web",
    "authors": [],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "botman/studio-addons": "~1.0",
        "illuminate/contracts": "~5.5.0",
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "dev-master",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Drivers\\Web\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Drivers\\Web\\Providers\\WebServiceProvider"
            ]
        }
    }
}
